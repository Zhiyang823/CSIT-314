<!DOCTYPE html>
<html lang="en">

<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="../menupagecss.css">
  <title>Staff Menu</title>
</head>

<body>
  <nav class="navbar navbar-expand-sm navbar-dark">
    <div class="container-fluid">
      <img class="icons" src="foodlogo.png">
      <div class="collapse navbar-collapse" id="mynavbar">
        <span class="navbar-nav me-auto">Unplugged</span>

              
        <button class="btn btn-light" type="button" style="margin-right: 90%;"><a href="/staff">BACK</a></button>
      </div>
    </div>
  </nav>


  <div class="table-orders-div">
    <div class="tableorders">
      <h2>Table 1</h2>
      <%orderList.forEach(order => {%>
        <ul>
            <li><%= order.itemId %> <%= order.itemPrice %></li>
        </ul>
        <%})%>
    </div>

    <div class="tableorders">
      <h2>Table 2</h2>
      <%orderList.forEach(order => {%>
        <ul>
            <li><%= order.itemId %> <%= order.itemPrice %></li>
        </ul>
        <%})%>
    </div>

    <div class="tableorders">
      <h2>Table 3</h2>
      <div id="tableordercontainer">
        <%orderList.forEach(order => {%>
          <ul>
              <li><%= order.itemId %> <%= order.itemPrice %></li>
          </ul>
          <%})%>
      </div>


      <div id="orderinfo">
        <button><a href="/editOrder">Edit Order</a></button><br>
        <button>Add Coupon Code</button><br>
        <span>Subtotal:</span><br>
        <span>GST inclusive:</span><br>
        <span>Total Amount:</span><br>
        <button>Confirm Order</button>
        <button>Confirm Payment</button>
      </div>



    </div>

    <div class="tableorders">
      <h2>Table 4</h2>
      <%orderList.forEach(order => {%>
        <ul>
            <li><%= order.itemId %> <%= order.itemPrice %></li>
        </ul>
        <%})%>
    </div>
  </div>
</body>

</html>